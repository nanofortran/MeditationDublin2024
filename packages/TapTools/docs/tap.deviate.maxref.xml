	</description>

	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">74 Objects</metadata>
		<metadata name="tag">TapTools</metadata>
		<metadata name="tag">Math</metadata>
	</metadatalist>

	
	<!--ARGUMENTS-->
	<objarglist>
		<objarg name="randomrange" type="float" optional="1">
			<description>Initial random range.</description>
		</objarg>
		<objarg name="scalerange" type="float" optional="1">
			<description>Initial scaling range.</description>
		</objarg>
	</objarglist>
	
		
	<!--SEEALSO-->
	<seealsolist>
		<seealso name="drunk"/>
	</seealsolist>

	
</c74object>
